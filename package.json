    "name": "bewoak",
    "nx": {
        "includedScripts": []
    },
    "private": true,
    "resolutions": {
        "callsites": "3.1.0"
    },
    "scripts": {
        "biome:check": "biome check --write",
        "build": "nx run-many --target=build --parallel=4",
        "format": "biome format --write && biome lint --write",
        "format:check": "biome format && biome lint",
        "nx": "nx",
        "postinstall": "chmod +x .hooks/* && git config core.hooksPath .hooks",
        "pretest": "rm -rf dist && rm -rf reports && bun run nx reset",
        "start:apps-pathway-design:server": "PORT=3000 GLOBAL_PREFIX=api PERSISTENCE_DRIVER=inMemory PRESENTER_DRIVER=toJson nx serve apps-pathway-design-server",
        "start:apps-search:client": "nx serve apps-search-client",
        "start:apps-search:server": "PERSISTENCE_DRIVER=inMemory PORT=3001 GLOBAL_PREFIX=api nx serve apps-search-server",
        "start:apps-shell:server": "PORT=3000 GLOBAL_PREFIX=api PERSISTENCE_DRIVER_PATHWAY_DESIGN_SERVER=inMemory PRESENTER_DRIVER_PATHWAY_DESIGN_SERVER=toJson PERSISTENCE_DRIVER_SEARCH_SERVER=inMemory nx serve apps-shell-server",
        "stryker": "stryker",
        "test": "bun test && bun run test:feature && bun run test:e2e && bun run build",
        "test:e2e": "nx run-many --target=test-e2e --parallel=4",
        "test:feature": "nx run-many --target=test-feature --parallel=4",
        "test:load-testing": "nx run-many --target=load-testing --output-style=static",
        "test:mutation": "nx run-many --target=test-mutation --parallel=4",
        "tools:autocanon": "autocannon -c 10 -d 10s http://localhost:3000/api/pathway/initialize",
        "tools:detective": "detective",
        "tools:otel:run": "docker compose -f ./docker/otel-instrumentation/docker-compose.yml up -d"
    },
    "version": "0.0.1"
}
