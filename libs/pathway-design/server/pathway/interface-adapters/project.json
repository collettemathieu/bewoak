{
    "$schema": "../../../../../../../node_modules/nx/schemas/project-schema.json",
    "name": "pathway-design-server-pathway-interface-adapters",
    "projectType": "library",
    "release": {
        "version": {
            "currentVersionResolver": "git-tag",
            "manifestRootsToUpdate": ["dist/{projectRoot}"],
            "preserveLocalDependencyProtocols": false
        }
    },
    "sourceRoot": "libs/pathway-design/server/pathway/interface-adapters/src",
    "tags": ["type:pathway-design:server:pathway:interface-adapters"],
    "targets": {
        "build": {
            "executor": "@nx/js:swc",
            "options": {
                "assets": [],
                "main": "libs/pathway-design/server/pathway/interface-adapters/src/index.ts",
                "outputPath": "dist/libs/pathway-design/server/pathway/interface-adapters",
                "tsConfig": "libs/pathway-design/server/pathway/interface-adapters/tsconfig.lib.json"
            },
            "outputs": ["{options.outputPath}"]
        },
        "nx-release-publish": {
            "options": {
                "packageRoot": "dist/{projectRoot}"
            }
        }
    }
}
