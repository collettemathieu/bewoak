{
    "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
    "name": "test-component",
    "prefix": "lib",
    "projectType": "library",
    "release": {
        "version": {
            "currentVersionResolver": "git-tag",
            "fallbackCurrentVersionResolver": "disk",
            "manifestRootsToUpdate": ["dist/{projectRoot}"]
        }
    },
    "sourceRoot": "libs/search/client/pathway/src",
    "tags": [],
    "targets": {
        "build": {
            "configurations": {
                "development": {},
                "production": {
                    "tsConfig": "libs/search/client/pathway/tsconfig.lib.prod.json"
                }
            },
            "defaultConfiguration": "production",
            "executor": "@nx/angular:package",
            "options": {
                "project": "libs/search/client/pathway/ng-package.json",
                "tsConfig": "libs/search/client/pathway/tsconfig.lib.json"
            },
            "outputs": ["{workspaceRoot}/dist/{projectRoot}"]
        },
        "nx-release-publish": {
            "options": {
                "packageRoot": "dist/{projectRoot}"
            }
        }
    }
}
